{
  "hosting": {
    "appAssociation": "NONE",
    "public": "public",
    "ignore": [
      "firebase.json",
      "**/.*",
      "**/node_modules/**"
    ],
    "rewrites": [
      {
        "source": "/.well-known/apple-app-site-association",
        "destination": "/.well-known/apple-app-site-association.json"
      }
    ]
  }
}